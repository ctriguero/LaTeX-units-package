\RequirePackage{xcolor}
\usepackage{mathtools}
%
%
% Kinematics units
%
%----------------------------------
% Space/length
%----------------------------------
% metres SI
\newcommand{\msis}[1]{#1 \textcolor{red!40}{\text{ m}}}
% centimetres cgs
\newcommand{\cmcgs}[1]{#1 \textcolor{red!40}{\text{ m}}}
% millimetres mm
\newcommand{\mm}[1]{#1 \textcolor{red!40}{\text{ m}}}
% terrestrial miles mi
\newcommand{\mi}[1]{#1 \textcolor{red!40}{\text{ mi}}}
% nautical miles
\newcommand{\nmi}[1]{#1 \textcolor{red!40}{\text{ nmi}}}
% light-year
\newcommand{\lightyear}[1]{#1 \textcolor{red!40}{\text{ ly}}}
% parsec
\newcommand{\parsec}[1]{#1 \textcolor{red!40}{\text{ pc}}}
%
%----------------------------------
% Space^2/length^2
%----------------------------------
% metres^2 SI
\newcommand{\msisA}[1]{#1 \textcolor{red!40}{\text{ m}^2}}
% centimetres^2 cgs
\newcommand{\cmcgsA}[1]{#1 \textcolor{red!40}{\text{ cm}^2}}
% millimetres^2
\newcommand{\mmA}[1]{#1 \textcolor{red!40}{\text{ mm}^2}}
% terrestrial miles^2
\newcommand{\miA}[1]{#1 \textcolor{red!40}{\text{ mi}^2}}
%
%----------------------------------
% Space^3/length^3
%----------------------------------
% metres^3 SI
\newcommand{\msisV}[1]{#1 \textcolor{red!40}{\text{ m}^3}}
% centimetres^3 cgs
\newcommand{\cmcgsV}[1]{#1 \textcolor{red!40}{\text{ m}^3}}
% millimetres^3
\newcommand{\mmV}[1]{#1 \textcolor{red!40}{\text{ m}^3}}
% terrestrial miles^3
\newcommand{\miV}[1]{#1 \textcolor{red!40}{\text{ mi}^3}}
% litre
\newcommand{\litre}[1]{#1 \textcolor{red!40}{\text{ l}}}
% gallon
\newcommand{\gallon}[1]{#1 \textcolor{red!40}{\text{ gal}}}
%----------------------------------
% Time
%----------------------------------
% second SI
\newcommand{\ssis}[1]{#1 \textcolor{red!40}{\text{ s}}}
\newcommand{\tsis}[1]{#1 \textcolor{red!40}{\text{ s}}}
% minute cgs
\newcommand{\mincgs}[1]{#1 \textcolor{red!40}{\text{ min}}}
% hour
\newcommand{\hours}[1]{#1 \textcolor{red!40}{\text{ h}}}
% day
\newcommand{\days}[1]{#1 \textcolor{red!40}{\text{ day}}}
% month
\newcommand{\months}[1]{#1 \textcolor{red!40}{\text{ month}}}
% year
\newcommand{\years}[1]{#1 \textcolor{red!40}{\text{ yr}}}
%
%----------------------------------
% Velocity/speed
%----------------------------------
% metres/second SI
\newcommand{\vsis}[1]{#1 \textcolor{red!40}{\text{ m}\cdot\text{s}^{-1}}}
% kilometres/hour
\newcommand{\kmh}[1]{#1 \textcolor{red!40}{\text{ m}\cdot\text{h}^{-1}}}
% terrestrial miles/hour
\newcommand{\mih}[1]{#1 \textcolor{red!40}{\text{ mi}\cdot\text{s}^{-1}}}
% nautical miles/hour
\newcommand{\knots}[1]{#1 \textcolor{red!40}{\text{ kn}}}
% centimetres/minute cgs
\newcommand{\vcgs}[1]{#1 \textcolor{red!40}{\text{ cm}\cdot\text{min}^{-1}}}
%
%----------------------------------
% Acceleration/rate of change of speed
%----------------------------------
% metres/second^2 SI
\newcommand{\asis}[1]{#1 \textcolor{red!40}{\text{ m}\cdot\text{s}^{-2}}}
% centimetres/minute^2 cgs
\newcommand{\acgs}[1]{#1 \textcolor{red!40}{\text{ cm}\cdot\text{min}^{-2}}}
% kilometres/hour^2
\newcommand{\akmh}[1]{#1 \textcolor{red!40}{\text{ m}\cdot\text{h}^{-2}}}
% terrestrial miles/hour
\newcommand{\amih}[1]{#1 \textcolor{red!40}{\text{ mi}\cdot\text{s}^{-2}}}
% nautical miles/hour^2
\newcommand{\anmi}[1]{#1 \textcolor{red!40}{\text{ nmi}\cdot\text{h}^{-2}}}
%
%
% Dynamics units
%
%----------------------------------
% Mass
%----------------------------------
% kilogram SI
\newcommand{\kgsis}[1]{#1 \textcolor{red!40}{\text{ kg}}}
% gram cgs
\newcommand{\gcgs}[1]{#1 \textcolor{red!40}{\text{ g}}}
\newcommand{\gram}[1]{#1 \textcolor{red!40}{\text{ g}}}
% tonne
\newcommand{\tonne}[1]{#1 \textcolor{red!40}{\text{ t}}}
% pound
\newcommand{\pound}[1]{#1 \textcolor{red!40}{\text{ lb}}}
\newcommand{\lb}[1]{#1 \textcolor{red!40}{\text{ lb}}}
%
%----------------------------------
% Linear momentum
%----------------------------------
% p SI
\newcommand{\psis}[1]{#1 \textcolor{red!40}{\text{ kg}\cdot\text{m}\cdot\text{s}^{-1}}}
% p SI
\newcommand{\pcgs}[1]{#1 \textcolor{red!40}{\text{ g}\cdot\text{cm}\cdot\text{s}^{-1}}}
%
%----------------------------------
% Impulse
%----------------------------------
% p SI
\newcommand{\Isis}[1]{#1 \textcolor{red!40}{\text{ kg}\cdot\text{m}\cdot\text{s}^{-1}}}
% p SI
\newcommand{\Icgs}[1]{#1 \textcolor{red!40}{\text{ g}\cdot\text{cm}\cdot\text{s}^{-1}}}
%
%----------------------------------
% Force
%----------------------------------
% Newton SI
\newcommand{\fsis}[1]{#1 \textcolor{red!40}{\text{ N}}}
\newcommand{\Nsis}[1]{#1 \textcolor{red!40}{\text{ N}}}
% dyne cgs
\newcommand{\fcgs}[1]{#1 \textcolor{red!40}{\text{ dyn}}}
\newcommand{\dyne}[1]{#1 \textcolor{red!40}{\text{ dyn}}}
%
%----------------------------------
% Energy/work
%----------------------------------
% Joule SI
\newcommand{\ensis}[1]{#1 \textcolor{red!40}{\text{ J}}}
\newcommand{\joule}[1]{#1 \textcolor{red!40}{\text{ J}}}
% erg cgs
\newcommand{\ergcgs}[1]{#1 \textcolor{red!40}{\text{ erg}}}
% electron-volt eV
\newcommand{\enev}[1]{#1 \textcolor{red!40}{\text{ eV}}}
%
%----------------------------------
% Moment/Torque of a force
%----------------------------------
% Moment of a force SI
\newcommand{\Msis}[1]{#1 \textcolor{red!40}{\text{ N}\cdot\text{m}}}
\newcommand{\Momentsis}[1]{#1 \textcolor{red!40}{\text{ N}\cdot\text{m}}}
%
%----------------------------------
% Power
%----------------------------------
% Watt SI
\newcommand{\Wsis}[1]{#1 \textcolor{red!40}{\text{ W}}}
\newcommand{\Powersis}[1]{#1 \textcolor{red!40}{\text{ W}}}
% erg cgs
\newcommand{\ergcgs}[1]{#1 \textcolor{red!40}{\text{ erg}\cdot\text{s}^{-1}}}
\newcommand{\Powercgs}[1]{#1 \textcolor{red!40}{\text{ erg}\cdot\text{s}^{-1}}}
% electron-volt eV
\newcommand{\Powerev}[1]{#1 \textcolor{red!40}{\text{ eV}\cdot\text{s}^{-1}}}




